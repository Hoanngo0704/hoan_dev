{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/web/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}